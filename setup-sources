#!/bin/sh

cd ..
git clone git@github.com:linuxmint/cinnamon.git
git clone git@github.com:linuxmint/muffin.git
git clone git@github.com:linuxmint/nemo.git
git clone git@github.com:linuxmint/cinnamon-session.git
git clone git@github.com:linuxmint/cinnamon-desktop.git
git clone git@github.com:linuxmint/cinnamon-settings-daemon.git
git clone git@github.com:linuxmint/cinnamon-control-center.git
git clone git@github.com:linuxmint/cinnamon-menus.git
git clone git@github.com:linuxmint/cjs.git
git clone git@github.com:linuxmint/mint-themes.git
git clone git@github.com:linuxmint/mdm.git
git clone git@github.com:linuxmint/mint-mdm-themes.git
git clone git@github.com:linuxmint/cinnamon-screensaver.git
git clone git@github.com:linuxmint/mint-themes-gtk3.git
git clone git@github.com:linuxmint/mint-x-icons.git
git clone git@github.com:linuxmint/cinnamon-translations.git
git clone git@github.com:linuxmint/cinnamon-themes.git
gsettings set com.linuxmint.git-monkey repos ["/home/mtwebster/bin/cinnamon:origin:master:origin","/home/mtwebster/bin/muffin:origin:master:origin","/home/mtwebster/bin/nemo:origin:master:origin","/home/mtwebster/bin/cinnamon-session:origin:master:origin","/home/mtwebster/bin/cinnamon-desktop:origin:master:origin","/home/mtwebster/bin/cinnamon-settings-daemon:origin:master:origin","/home/mtwebster/bin/cinnamon-control-center:origin:master:origin","/home/mtwebster/bin/cinnamon-menus:origin:master:origin","/home/mtwebster/bin/cjs:origin:master:origin","/home/mtwebster/bin/mint-themes:origin:master:origin","/home/mtwebster/bin/mdm:origin:master:origin","/home/mtwebster/bin/mint-mdm-themes:origin:master:origin","/home/mtwebster/bin/cinnamon-screensaver:origin:master:origin","/home/mtwebster/bin/mint-themes-gtk3:origin:master:origin","/home/mtwebster/bin/mint-x-icons:origin:master:origin","/home/mtwebster/bin/cinnamon-translations:origin:master:origin","/home/mtwebster/bin/cinnamon-themes:origin:master:origin"]
